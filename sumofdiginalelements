#include <iostream>
#include <vector>
using namespace std;
int main()
{
    vector<vector<int>>v(10,vector<int>(10));
    for(int i=0;i<10;i++)
    for(int j=0;j<10;j++)
    cin>>v[i][j];
    int sum = 0;
    for(int i=0;i<10;i++)
    {
        sum = sum+v[i][i];
    }
    cout<<sum;
    return 0;
}